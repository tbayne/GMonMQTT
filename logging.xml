<seelog minlevel="trace" maxlevel="critical">
	<outputs formatid="sw1">
                <console formatid="console"></console>
		<rollingfile type="size" filename="/tmp/x_bridge_node.log" maxsize="1000000" maxrolls="50" />
	</outputs>

	<formats>
		<format id="sw1" format="[%LEV] (%Date %Time) [%File:%Func] %Msg%n"/>
        <format id="console" format="%Ns:%FuncShort : %Msg%n"/>
	</formats>
</seelog>